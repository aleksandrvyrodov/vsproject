{
  /* "terminal.integrated.profiles.windows": {
        "PowerShell -NoProfile": {
            "source": "PowerShell",
            "args": [
                "-NoProfile"
            ]
        }
    }, */
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "files.autoSave": "off",
  "files.autoGuessEncoding": true,
  "editor.fontFamily": "'Fira Code',Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "300",
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "Wscats.eno"
  },
  "intelephense.environment.phpVersion": "7.4.0",
  "php.validate.executablePath": "W:\\modules\\php\\PHP_7.4\\php.exe",
  // "phpcs.executablePath": "W:\\userdata\\composer\\vendor\\bin\\phpcs",
  "php.executablePath": "W:\\modules\\php\\PHP_7.4\\php.exe",
  "svg.preview.mode": "img",
  "todo-tree.tree.showScanModeButton": true,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.highlights.customHighlight": {
    "FAR": {
      "foreground": "#577ee9",
      "background": "#ffffff",
      "fontWeight": "700",
      "type": "text"
    },
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "FAR"
  ],
  "files.associations": {
    "*.twig": "twig"
  },
  "redhat.telemetry.enabled": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.tabSize": 2
}