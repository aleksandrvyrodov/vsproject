[
    {
        "key": "ctrl+space numpad_divide",
        "command": "editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+a",
        "command": "-editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+q tab",
        "command": "editor.emmet.action.expandAbbreviation",
        "when": "config.emmet.triggerExpansionOnTab && editorFocus && editorHasSelection && editorTextFocus && !editorReadonly && !editorTabMovesFocus"
    },
    {
        "key": "alt+q u",
        "command": "editor.emmet.action.updateTag"
    },
	{
        "key": "alt+q q",
        "command": "workbench.action.showEmmetCommands"
    },
	{
        "key": "alt+q up",
        "command": "editor.emmet.action.balanceOut"
    },
    {
        "key": "alt+q down",
        "command": "editor.emmet.action.balanceIn"
    },
    {
        "key": "alt+q w",
        "command": "editor.emmet.action.wrapWithAbbreviation"
    },
    {
        "key": "alt+q r",
        "command": "editor.emmet.action.removeTag"
    },
	{
        "key": "alt+q s",
        "command": "editor.emmet.action.splitJoinTag"
    },
    {
        "key": "ctrl+numpad_divide",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+/",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+space space",
        "command": "compile-hero.compileFile"
    }
]